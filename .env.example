# Rename this file to .env and fill with your credentials.
# Do NOT commit .env to Git.

# Bitget (optional for live trading & balances)
BITGET_KEY=
BITGET_SECRET=
BITGET_PASSWORD=

# OpenAI (optional for AI market analysis)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini

# Gemini (optional; free tier friendly for fallback/rotation)
GEMINI_API_KEY=
GEMINI_MODEL=gemini-1.5-flash

# xAI Grok (optional; OpenAI-compatible)
GROK_API_KEY=
GROK_MODEL=grok-4-latest

# AI Orchestrator (limits & routing)
AI_PROVIDER_ORDER=openai,gemini,grok
OPENAI_MAX_RPM=
OPENAI_MAX_TPM=
OPENAI_MAX_CONCURRENCY=3
GEMINI_MAX_RPM=
GEMINI_MAX_TPM=
GEMINI_MAX_CONCURRENCY=3
GROK_MAX_RPM=
GROK_MAX_TPM=
GROK_MAX_CONCURRENCY=3
AI_MIN_INTERVAL_1M=60
AI_MIN_INTERVAL_5M=300
AI_MIN_INTERVAL_15M=900
AI_DEBOUNCE_MS=400
AI_MAX_TOKENS_FAST=256
AI_MAX_TOKENS_DEEP=768

# Runtime knobs (safe defaults)
LIVE_TRADING=false
NEWS_ENABLE=true
IND_MAX_LEN=800
PLOT_BARS=320
DF_FLOAT_DTYPE=float32
DEBUG_LOG=false
